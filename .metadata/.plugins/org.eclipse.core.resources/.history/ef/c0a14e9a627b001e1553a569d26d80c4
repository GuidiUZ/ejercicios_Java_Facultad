package arrayLists;

public class Persona {

	private String name;
	private int num;
	private int edad;
	
	public Persona() {
		
	}
	
	public Persona(String name, int num, int edad) {
		this.name = name;
		this.num = num;
		this.edad = edad;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public int getEdad() {
		return edad;
	}

	public void setEdad(int edad) {
		this.edad = edad;
	}
	
	
	
	
}
